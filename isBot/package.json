{
  "name": "@rearguard/is-bot",
  "version": "1.9.4",
  "description": "Ultra fast bot/crawler detector using user agent strings.",
  "license": "MIT",
  "private": false,
  "main": "dist/index.js",
  "module": "dist/index.m.js",
  "types": "index.d.ts",
  "keywords": [
    "utility",
    "bot",
    "user-agent",
    "validator"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/proximahq/rearguard.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "rm -rf dist && rollup -c ../rollup.config.js --environment production",
    "test": "ava",
    "test:watch": "ava --watch",
    "bump": "yarn publish --new-version $PACKAGE_VERSION",
    "version": "yarn version --new-version $PACKAGE_VERSION --no-git-tag-version --no-commit-hooks"
  },
  "ava": {
    "require": [
      "esm"
    ]
  }
}
