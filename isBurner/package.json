{
  "name": "@rearguard/is-burner",
  "version": "1.0.0",
  "description": "A fast email burner checker.",
  "license": "MIT",
  "private": false,
  "main": "dist/index.js",
  "module": "dist/index.m.js",
  "repository": {
    "type": "git",
